.input-field input[type=search] {
    padding-left: 0rem !important;
    width: calc(100% - 5rem) !important;
    margin-bottom: 0px !important;
}


}

.input-field input[type=search] {
    display: block;
    line-height: inherit;
    padding-left: 4rem;
    width: calc(100% - 10rem);
}